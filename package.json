{
    "name": "cache-keeper",
    "version": "1.0.0",
    "homepage": "https://github.com/DevVali/cache-keeper",
    "repository": "https://github.com/DevVali/cache-keeper",
    "author": "DevVali",
    "description": "A simple and lightweight caching solution for your Discord bot.",
    "main": "./index.js",
    "devDependencies": {
        "@stylistic/eslint-plugin": "latest",
        "@types/bun": "latest",
        "esbuild": "^0.23.0",
        "eslint": "latest",
        "eslint-plugin-jsdoc": "latest",
        "mitata": "latest",
        "typescript": "latest",
        "typescript-eslint": "latest"
    },
    "keywords": [
        "caching",
        "cache",
        "cache-storage",
        "discord-bot",
        "discord-js"
    ],
    "license": "MIT",
    "scripts": {
        "build:test": "bun scripts/build.ts && bun test",
        "build:publish": "bun build:test && bun scripts/publish.ts",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix"
    },
    "type": "module",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "import": "./index.js",
            "require": "./index.cjs"
        }
    },
    "dependencies": {
        "@discordjs/collection": "^2.1.0"
    }
}
